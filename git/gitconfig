[advice]
	statusHints = false
[alias]
	amend = commit --amend
	ci = commit
	civ = commit --verbose
	co = checkout
	d = diff
	dc = diff --cached
	remote = remote --verbose
	s = status
	sl = stash list
	st = status
	unstage = reset HEAD
[branch]
	autosetupmerge = always
	autosetuprebase = always
[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore_global
[credential]
	helper = osxkeychain
[hub]
	protocol = https
[push]
	default = simple
