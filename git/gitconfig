[alias]
	amend = commit --amend
	ci = commit
	co = checkout
	d = diff
	dc = diff --cached
	remote = remote -v
	sl = stash list
	st = status
[branch]
	autosetupmerge = always
	autosetuprebase = always
[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore_global
[credential]
	helper = osxkeychain
[hub]
	protocol = https
[push]
	default = simple
